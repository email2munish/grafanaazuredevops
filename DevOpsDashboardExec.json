      "targets": [
        {
          "dataset": "devopsdatabase",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH ArtifactPromotionFlags AS (\r\n  SELECT\r\n    cdt.artifact_id,\r\n    DATEADD(WEEK, DATEDIFF(WEEK, 0, MIN(cdt.cd_date)), 0) AS WeekStart,\r\n    MAX(CASE WHEN cdt.env_level = 'DEV' AND cdt.deployment_status = 'SUCCESS' THEN 1 ELSE 0 END) AS has_dev,\r\n    MAX(CASE WHEN cdt.env_level = 'QA' AND cdt.deployment_status = 'SUCCESS' THEN 1 ELSE 0 END) AS has_staging,\r\n    MAX(CASE WHEN cdt.env_level = 'PROD' AND cdt.deployment_status = 'SUCCESS' THEN 1 ELSE 0 END) AS has_prod\r\n  FROM devopsdata.CDT_Metrics cdt\r\n  JOIN devopsdata.CI_Metrics ci ON cdt.artifact_id = ci.artifact_id\r\n  WHERE\r\n    cdt.cd_date BETWEEN $__timeFrom() AND $__timeTo()\r\n  AND (ci.group_name IN ($GroupName))\r\n  AND (ci.area_name IN ($AreaName))\r\n  AND (ci.service_name IN ($ServiceName))\r\n  GROUP BY cdt.artifact_id\r\n)\r\n\r\nSELECT\r\n  WeekStart,\r\n  COUNT(*) AS TotalArtifactsInDev,\r\n  SUM(CASE WHEN has_dev = 1 AND has_staging = 1 AND has_prod = 1 THEN 1 ELSE 0 END) AS FullyPromotedArtifacts,\r\n  CAST(\r\n    100.0 * SUM(CASE WHEN has_dev = 1 AND has_staging = 1 AND has_prod = 1 THEN 1 ELSE 0 END)\r\n    / NULLIF(COUNT(*), 0)\r\n    AS DECIMAL(5,2)\r\n  ) AS ArtifactPromotionRate\r\nFROM ArtifactPromotionFlags\r\nWHERE has_dev = 1\r\nGROUP BY WeekStart\r\nORDER BY WeekStart;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "devopsdata.CDT_Metrics"
        }
      ],
      "title": "Artifact Promotion Rate",
      "type": "timeseries"
    },
